{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vmName": { "value": "#vmname#" },
    "publicDnsPrefix": { "value": "peterlil-#vmname#" },
    "vmSize": {"value": "#vmSize#"},
    "storageType": {"value": "Premium_LRS"},
    "vnetName": { "value": "#vnetname#" },
    "subnetName": { "value": "#subnetname#" },
    "enableAcceleratedNetworking": {
        "value": #enableAcceleratedNetworking#
    },
    "networkSecurityGroupRules": {
        "value": []
    },
    "adminUserName": { "value": "#adminusername#" },
    "keyVaultName": { "value": "#keyvaultname#" },
    "keyVaultResourceGroup": { "value": "#keyvaultresourcegroup#" },
    "aadClientID": {
      "value": "#aadClientID#"
    },
    "aadClientSecret": {
      "value": "#aadClientSecret#"
    },
    "autoShutdownStatus": {
        "value": "Enabled"
    },
    "autoShutdownTime": {
        "value": "17:00"
    },
    "autoShutdownTimeZone": {
        "value": "W. Europe Standard Time"
    },
    "autoShutdownNotificationStatus": {
        "value": "Enabled"
    },
    "autoShutdownNotificationLocale": {
        "value": "en"
    },
    "autoShutdownNotificationEmail": {
        "value": "#autoShutdownNotificationEmail#"
    }
  }
}
